    "docker:run": "docker run -p 3000:3000 -p 8000:8000 --gpus all image-generator-ai:latest"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "next": "^14.0.0",
    "typescript": "^5.3.3",
