{
  "name": "image-generator-ai",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "backend": "python -m uvicorn backend.main:app --reload --host 0.0.0.0 --port 8000",
    "backend-prod": "python -m uvicorn backend.main:app --host 0.0.0.0 --port 8000",
    "dev:all": "concurrently \"npm run dev\" \"npm run backend\"",
